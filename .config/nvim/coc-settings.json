{
  "eslint.enable": true,
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": ["javascript","javascriptreact", "typescript", "vue"],
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "javascriptreact", "typescript", "vue", "rust"],
  "javascript.validate.enable": false,
  "snippets.ultisnips.directories": [
    "~/.config/nvim/UltiSnips",
  ] ,
  "rust.clippy_preference":"on",
  "languageserver": {
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform"],
      "initializationOptions": {}
    },
    "ccls": {
      "command": "ccls",
      "filetypes": [
        "c",
        "cpp",
        "objc",
        "objcpp"
      ],
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    }
  }
}
