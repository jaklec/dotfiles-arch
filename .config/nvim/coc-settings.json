{
  "eslint.enable": true,
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": ["javascript","javascriptreact", "typescript", "vue"],
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "javascriptreact", "typescript", "vue", "rust", "svelte", "python"],
  "javascript.validate.enable": false,
  "python.jediEnabled": false,
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "/usr/bin/black",
  "snippets.ultisnips.directories": [
    "~/.config/nvim/UltiSnips"
  ] ,
  "languageserver": {
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform"],
      "initializationOptions": {}
    },
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    },
    "kotlin": {
      "command": "/usr/bin/kotlin-language-server",
      "filetypes": ["kotlin"]
    }
  },
  "rust-analyzer.serverPath":"/usr/bin/rust-analyzer",
  "java.home":"/usr/lib/jvm/default",
  "cSpellExt.enableDictionaries": ["swedish"],
  "cSpell.language": "en,sv",
  "cSpell.userWords": [
    "Leczinsky",
    "Millis",
    "etag",
    "fastify",
    "healthcheck",
    "postgres"
  ]
}
